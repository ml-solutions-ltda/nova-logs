{
    "name": "mlsolutions/nova-logs",
    "description": "Provides a Log Viewer for Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "ML Solutions",
            "email": "ti@mlsolutions.net.br"
        }
    ],
    "require": {
        "php": "^8.0|^8.3",
        "laravel/nova": "^4.0|^5.0",
        "arcanedev/log-viewer": "develop as 11.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "MlSolutions\\NovaLogViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MlSolutions\\NovaLogViewer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MlSolutions\\NovaLogViewer\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
